<p id="notice"><%= notice %></p>

<h1>Scoreboard</h1>


  <table cellpadding="10" class="table-bordered table-striped">
  <thead>
    <tr>
      
      <th>  Name  </th>
      <th>  School  </th>
      <th>  Score  </th>
      
    </tr>
  </thead>

  <tbody>
      <% @users.each do |s| %>
      <tr>
          <td><%= s.scoreboard_name %></td>
          <td><%= s.scoreboard_school %></td>
          <td><%= s.print_score %></td>
      </tr>
      <%end%>
  </tbody>
</table>

<br>

