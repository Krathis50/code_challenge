<p id="notice"><%= notice %></p>

<h1>Scoreboard</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
      <th>Name </th>
      <th>School </th>
      <th>Score </th>
      
    </tr>
  </thead>

  <tbody>
    <% @scoreboards.each do |scoreboard| %>
      <tr>
          <%= scoreboards.name %></td>
          <%= scoreboards.score %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


